{
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@assets/*": ["./src/assets/*"],
      "@components/*": ["./src/components/*"],
      "@components": ["./src/components"],
      "@hooks": ["./src/hooks"],
      "@hooks/*": ["./src/hooks/*"],
      "@config/*": ["./src/config/*"],
      "@navigation/*": ["./src/navigation/*"],
      "@navigation": ["./src/navigation"],
      "@pages/*": ["./src/pages/*"],
      "@services/*": ["./src/services/*"],
      "@services": ["./src/services"],
      "@utils/*": ["./src/utils/*"],
      "@store/*": ["./src/store/*"],
      "@store": ["./src/store"],
    }
  },
  "extends": "expo/tsconfig.base.json",
}
